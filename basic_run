ansible testserver -i inventory/vagrant.ini -m ping                                          [webservers:vars]
ansible testserver -i hosts.ini -m command -a uptime 
ansible testserver -i hosts.ini -m command -a "tail /var/log/lastlog"                                                 ansible_user=ec2-user
                                                                                             ansible_private_key_file=/path/to/keyfile.pem

ansible testserver -i hosts.ini -m command -b -a "tail /var/log/syslog" -K   --root(-b become) and interactive password(-K)
 
